package objectsAndInterfaces;

public class Book extends Item {
	private String author;
	
	public Book() {
		setTitle("");
		author = "";
	}
	
	public Book(String title, String author) {
		setTitle(title);
		this.author = author;
	}
	
	public int compareTo() {
		return 0;
	}

	public String print() {
		return "Title: " + getTitle() + ", Author: " + author;
	}
	
}
